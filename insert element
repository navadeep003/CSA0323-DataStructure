#include <stdio.h>
int main()
{
	int i,a[15],n,x,j,ele;
	printf("The number of elements ");
	scanf("%d",&n);
	printf("Enter the elements in sorted order :");
	for(i=0;i<n;i++)
	{
	scanf("%d",&a[i]);
	}
	printf("Enter the number to be inserted");
	scanf("%d",&ele);
	printf("Enter the position to be inserted");
	scanf("%d",&x);
	for(j-n;j>=x;j--1)
	{
		a[j+1]=a[j]]
	}
		a[j+1]=ele;
		for(j=0;j<a[x];j++)
		{
			printf("%d",a[ji])
			printf("\n");
	}
}
